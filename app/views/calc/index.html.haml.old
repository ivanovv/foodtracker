- title "Calc"

%table
  %tr
    %th Weight
  - for calc in @calcs
    %tr
      %td= h calc.weight
      %td= link_to 'Show', calc
      %td= link_to 'Edit', edit_calc_path(calc)
      %td= link_to 'Destroy', calc, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New Calc", new_calc_path
